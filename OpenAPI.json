    "/calendar/{season}": {
      "get": {
        "tags": [
          "Season"
        ],
        "summary": "Get Calendar By Season",
        "operationId": "get_calendar_by_season",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Season",
              "type": "integer"
            },
            "name": "season",
            "in": "path"
