                          "long": 50.5106,
                          "locality": "Sakhir",
                          "country": "Bahrain"
                        }
                      },
                      "date": "2022-03-20",
                      "time": "15:00:00Z",
                      "FirstPractice": {
                        "date": "2022-03-18",
                        "time": "12:00:00Z"
                      },
                      "SecondPractice": {
                        "date": "2022-03-18",
                        "time": "15:00:00Z"
                      },
                      "ThirdPractice": {
                        "date": "2022-03-19",
                        "time": "12:00:00Z"
                      },
                      "Qualifying": {
                        "date": "2022-03-19",
                        "time": "15:00:00Z"
                      }
                    }
                  ]
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Message"
                },
                "example": {
                  "message": "Calendar not found"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
